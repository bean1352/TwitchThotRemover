{
    "manifest_version": 3,
    "name": "Twitch thot remover",
    "version": "1.0.0",
    "icons": {
        "16": "public/icon.png",
        "32": "public/icon.png",
        "48": "public/icon.png",
        "128": "public/icon.png"
    },
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "js": [
                "src/content.js"
            ],
            "matches": [
                "https://www.twitch.tv/*"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}