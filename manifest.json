{
  "manifest_version": 2,
  "name": "Thot Remover",
  "version": "1.0",
  "description": "Description of your extension",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
